<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/reno#"
     xml:base="https://w3id.org/reno"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:ns="http://www.w3.org/2003/06/sw-vocab-status/ns#"
     xmlns:ns1="http://www.w3.org/2006/vcard/ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:wot="http://xmlns.com/wot/0.1/"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:reno="https://w3id.org/reno#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:schema="https://schema.org/">
    <owl:Ontology rdf:about="https://w3id.org/reno#">
        <owl:imports rdf:resource="http://www.daml.org/services/owl-s/1.1/Process.owl"/>
        <owl:imports rdf:resource="http://xmlns.com/foaf/0.1/"/>
        <owl:imports rdf:resource="https://bimerr.iot.linkeddata.es/def/material-properties/ontology.ttl"/>
        <owl:imports rdf:resource="https://pi.pauwel.be/voc/buildingelement"/>
        <owl:imports rdf:resource="https://saref.etsi.org/saref4bldg/v2.1.1/"/>
        <owl:imports rdf:resource="https://w3id.org/bot"/>
        <terms:abstract xml:lang="nl">Deze ontologie werd ontwikkeld als onderdeel van het RE-ENNOVATE project. Het doel is om concepten vast te leggen, die relevant zijn bij het ontwerpen van energetische renovatie suggesties.</terms:abstract>
        <terms:abstract xml:lang="en">This ontology was developed as part of the RE-ENNOVATE project. It aims to capture concepts that are relevant when designing energy renovation suggestions.</terms:abstract>
        <terms:creator>Emma Nuyts</terms:creator>
        <terms:creator>Femke Ongenae</terms:creator>
        <terms:creator>Mathias De Brouwer</terms:creator>
        <terms:license>https://creativecommons.org/licenses/by/4.0/</terms:license>
        <terms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-07-14</terms:modified>
        <terms:title xml:lang="nl">Renovatie en ENergie Optimalisatie (RENO)</terms:title>
        <terms:title xml:lang="en">Renovation and ENergy Optimization (RENO)</terms:title>
        <vann:preferredNamespacePrefix>reno</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>https://w3id.org/reno#</vann:preferredNamespaceUri>
        <owl:versionInfo>1.0.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/terms/abstract -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/abstract"/>
    


    <!-- http://purl.org/dc/terms/contributor -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/contributor"/>
    


    <!-- http://purl.org/dc/terms/creator -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/creator"/>
    


    <!-- http://purl.org/dc/terms/license -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- http://purl.org/dc/terms/title -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/title"/>
    


    <!-- http://purl.org/vocab/vann/preferredNamespacePrefix -->


    <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/preferredNamespacePrefix"/>
    


    <!-- http://purl.org/vocab/vann/preferredNamespaceUri -->


    <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/preferredNamespaceUri"/>
    


    <!-- https://w3id.org/reno#hasNuisanceLevel -->


    <owl:AnnotationProperty rdf:about="https://w3id.org/reno#hasNuisanceLevel">
        <rdfs:comment xml:lang="nl">Geeft het overlastniveau van een renovatiesuggestie aan.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates the nuisance level of a renovation suggestion.</rdfs:comment>
        <rdfs:label xml:lang="en">has nuisance level</rdfs:label>
        <rdfs:label xml:lang="nl">heeft overlastniveau</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:domain rdf:resource="https://w3id.org/reno#RenovationSuggestion"/>
    </owl:AnnotationProperty>
    


    <!-- https://w3id.org/reno#hasPointOfAttention -->


    <owl:AnnotationProperty rdf:about="https://w3id.org/reno#hasPointOfAttention">
        <rdfs:comment xml:lang="nl">De relatie tussen een renovatie suggestie en zijn aandachtspunt(en).</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship between a renovation suggestion and its point(s) of attention.</rdfs:comment>
        <rdfs:label xml:lang="en">has point of attention</rdfs:label>
        <rdfs:label xml:lang="nl">heeft aandachtspunt</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:domain rdf:resource="https://w3id.org/reno#RenovationSuggestion"/>
    </owl:AnnotationProperty>
    


    <!-- https://w3id.org/reno#hasUnitPrice -->


    <owl:AnnotationProperty rdf:about="https://w3id.org/reno#hasUnitPrice">
        <rdfs:comment xml:lang="nl">Geeft de eenheidsprijs aan voor de renovatiesuggestie. Dit kan per stuk of per vierkante meter zijn, afhankelijk van het type renovatie.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates the unit price for the renovation suggestion. This can be per piece or per sqm, depending on the type of renovation.</rdfs:comment>
        <rdfs:label xml:lang="en">has unit price</rdfs:label>
        <rdfs:label xml:lang="nl">heeft eenheidsprijs</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:domain rdf:resource="https://w3id.org/reno#RenovationSuggestion"/>
    </owl:AnnotationProperty>
    


    <!-- https://w3id.org/reno#permitNeeded -->


    <owl:AnnotationProperty rdf:about="https://w3id.org/reno#permitNeeded">
        <rdfs:comment xml:lang="nl">Geeft aan of een omgevingsvergunning nodig is voor de renovatiesuggestie.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates whether or not a permit is required for the renovation suggestion.</rdfs:comment>
        <rdfs:label xml:lang="en">permit needed</rdfs:label>
        <rdfs:label xml:lang="nl">vergunning nodig</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:domain rdf:resource="https://w3id.org/reno#RenovationSuggestion"/>
    </owl:AnnotationProperty>
    


    <!-- https://w3id.org/reno#requiresMultiPhase -->


    <owl:AnnotationProperty rdf:about="https://w3id.org/reno#requiresMultiPhase">
        <rdfs:comment xml:lang="nl">Geeft aan of een renovatiesuggestie een meerfasige elektriciteitsaansluiting vereist.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates whether or not a renovation suggestion requires a multi-phase electricity connection.</rdfs:comment>
        <rdfs:label xml:lang="en">requires multi-phase</rdfs:label>
        <rdfs:label xml:lang="nl">vereist meerfasig</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:domain rdf:resource="https://w3id.org/reno#RenovationSuggestion"/>
    </owl:AnnotationProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/2001/XMLSchema#gYear -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#gYear"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#time -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#time"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://w3id.org/reno#changedDuring -->


    <owl:ObjectProperty rdf:about="https://w3id.org/reno#changedDuring">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/reno#RenovationIntervention"/>
        <rdfs:comment xml:lang="nl">De relatie tussen een bouwcomponent (set materiaallagen, raamkader...) en de renovatie interventie waarbij het veranderde.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship between a building component (material layer set, windowframe...) and the renovation intervention during which it changed.</rdfs:comment>
        <rdfs:label xml:lang="en">changed during</rdfs:label>
        <rdfs:label xml:lang="nl">veranderd tijdens</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reno#changedInto -->


    <owl:ObjectProperty rdf:about="https://w3id.org/reno#changedInto">
        <rdfs:comment xml:lang="nl">De relatie tussen een bouwdeel (set materiaallagen, raamkader...) en het bouwdeel waarin het veranderde.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship between a building component (material layer set, windowframe...) and the building component it changed into.</rdfs:comment>
        <rdfs:label xml:lang="en">changed into</rdfs:label>
        <rdfs:label xml:lang="nl">veranderd in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reno#clustering -->


    <owl:ObjectProperty rdf:about="https://w3id.org/reno#clustering">
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/spec/Person"/>
        <rdfs:range rdf:resource="https://w3id.org/reno#Persona"/>
        <rdfs:comment xml:lang="en">Links a person to their clustered persona.</rdfs:comment>
        <rdfs:comment xml:lang="nl">Linkt een persoon aan zijn geclusterde persona.</rdfs:comment>
        <rdfs:label xml:lang="en">clustering</rdfs:label>
        <rdfs:label xml:lang="nl">clustering</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reno#commodityAccomplishes -->


    <owl:ObjectProperty rdf:about="https://w3id.org/reno#commodityAccomplishes">
        <rdfs:domain rdf:resource="https://saref.etsi.org/core/Commodity"/>
        <rdfs:range rdf:resource="https://saref.etsi.org/core/Task"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://saref.etsi.org/core/isConsumedBy"/>
            <rdf:Description rdf:about="https://saref.etsi.org/core/accomplishes"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="nl">De relatie tussen een drager en de taak die erdoor wordt verwezenlijkt.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship between a commodity and the task it accomplishes.</rdfs:comment>
        <rdfs:label xml:lang="en">commodity accomplishes</rdfs:label>
        <rdfs:label xml:lang="nl">drager verwezenlijkt</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reno#forRenovationIntervention -->


    <owl:ObjectProperty rdf:about="https://w3id.org/reno#forRenovationIntervention">
        <rdfs:domain rdf:resource="https://w3id.org/reno#Tender"/>
        <rdfs:range rdf:resource="https://w3id.org/reno#RenovationIntervention"/>
        <rdfs:comment xml:lang="en">Links a tender to the renovation intervention to which the tender is related.</rdfs:comment>
        <rdfs:comment xml:lang="nl">Linkt een offerte aan de renovatie interventie waarop de offerte betrekking heeft.</rdfs:comment>
        <rdfs:label xml:lang="en">for renovation intervention</rdfs:label>
        <rdfs:label xml:lang="nl">voor renovatie interventie</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reno#hasAddress -->


    <owl:ObjectProperty rdf:about="https://w3id.org/reno#hasAddress">
        <owl:inverseOf rdf:resource="https://w3id.org/reno#isAddressOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/reno#Resident"/>
        <rdfs:range rdf:resource="https://w3id.org/reno#Address"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/reno#isResidentOf"/>
            <rdf:Description rdf:about="https://w3id.org/reno#hasAddress"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="nl">De relatie tussen een persoon en het adres waarop ze wonen.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship between a person and the address they reside at.</rdfs:comment>
        <rdfs:label xml:lang="en">has address</rdfs:label>
        <rdfs:label xml:lang="nl">heeft adres</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reno#hasAffectedElement -->


    <owl:ObjectProperty rdf:about="https://w3id.org/reno#hasAffectedElement">
        <owl:inverseOf rdf:resource="https://w3id.org/reno#isAffectedBy"/>
        <rdfs:domain rdf:resource="https://w3id.org/reno#Tender"/>
        <rdfs:range rdf:resource="https://w3id.org/bot#Element"/>
        <rdfs:comment xml:lang="nl">De relatie tussen een offerte en de (bouw)elementen die erdoor worden beïnvloed.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship between a tender and the (building) elements it affects.</rdfs:comment>
        <rdfs:label xml:lang="en">has affected element</rdfs:label>
        <rdfs:label xml:lang="nl">heeft invloed op element</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reno#hasCreator -->


    <owl:ObjectProperty rdf:about="https://w3id.org/reno#hasCreator">
        <owl:inverseOf rdf:resource="https://w3id.org/reno#isCreatorOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/reno#EnergyPerformanceCertificate"/>
        <rdfs:range rdf:resource="https://w3id.org/reno#EnergyExpert"/>
        <rdfs:comment xml:lang="nl">De relatie tussen een energieprestatiecertificaat en de energiedeskundige die het opgesteld heeft.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship between a energy performance certificate and the energy expert who created it.</rdfs:comment>
        <rdfs:label xml:lang="en">has creator</rdfs:label>
        <rdfs:label xml:lang="nl">heeft maker</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reno#hasDesiredSoundLevel -->


    <owl:ObjectProperty rdf:about="https://w3id.org/reno#hasDesiredSoundLevel">
        <rdfs:domain rdf:resource="https://w3id.org/bot#Space"/>
        <rdfs:range rdf:resource="https://w3id.org/reno#SoundLevel"/>
        <rdfs:comment xml:lang="nl">De relatie tussen een ruimte en zijn gewenst geluidsniveau.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship between a space and its desired sound level.</rdfs:comment>
        <rdfs:label xml:lang="en">has desired sound level</rdfs:label>
        <rdfs:label xml:lang="nl">heeft gewenst geluidsniveau</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reno#hasEnergyPerformanceCertificate -->


    <owl:ObjectProperty rdf:about="https://w3id.org/reno#hasEnergyPerformanceCertificate">
        <rdfs:domain rdf:resource="https://w3id.org/bot#Zone"/>
        <rdfs:range rdf:resource="https://w3id.org/reno#EnergyPerformanceCertificate"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/bot#containsZone"/>
            <rdf:Description rdf:about="https://w3id.org/reno#hasEnergyPerformanceCertificate"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="nl">De relatie tussen een zone en het bijhorende energieprestatiecertificaat. Een zone kan de energieprestatiecertificaten van de subzones aggregeren.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship between a zone and its energy performance certificate. A zone can aggregate the energy performance certificates of its subszones.</rdfs:comment>
        <rdfs:label xml:lang="en">has energy performance certificate</rdfs:label>
        <rdfs:label xml:lang="nl">heeft energieprestatiecertificaat</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reno#hasFrame -->


    <owl:ObjectProperty rdf:about="https://w3id.org/reno#hasFrame">
        <owl:inverseOf rdf:resource="https://w3id.org/reno#isFrameOf"/>
        <rdfs:domain rdf:resource="https://pi.pauwel.be/voc/buildingelement#Window"/>
        <rdfs:range rdf:resource="https://w3id.org/reno#WindowFrame"/>
        <rdfs:comment xml:lang="nl">De relatie tussen een raam en zijn kader.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship between a window and its frame.</rdfs:comment>
        <rdfs:label xml:lang="en">has frame</rdfs:label>
        <rdfs:label xml:lang="nl">heeft kader</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reno#hasFrameMaterial -->


    <owl:ObjectProperty rdf:about="https://w3id.org/reno#hasFrameMaterial">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/reno#WindowFrame"/>
        <rdfs:range rdf:resource="https://w3id.org/reno#WindowFrameMaterial"/>
        <rdfs:comment xml:lang="nl">De relatie tussen een raamkader en het materiaal waaruit het vervaardigt is.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship between a window frame and the material it is made of.</rdfs:comment>
        <rdfs:label xml:lang="en">frame material</rdfs:label>
        <rdfs:label xml:lang="nl">kader materiaal</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reno#hasGlazing -->


    <owl:ObjectProperty rdf:about="https://w3id.org/reno#hasGlazing">
        <owl:inverseOf rdf:resource="https://w3id.org/reno#isGlazingOf"/>
        <rdfs:domain rdf:resource="https://pi.pauwel.be/voc/buildingelement#Window"/>
        <rdfs:range rdf:resource="https://w3id.org/reno#WindowGlazing"/>
        <rdfs:comment xml:lang="nl">De relatie tussen een raam en zijn beglazing.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship between a window and its glazing.</rdfs:comment>
        <rdfs:label xml:lang="en">has glazing</rdfs:label>
        <rdfs:label xml:lang="nl">heeft beglazing</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reno#hasGlazingType -->


    <owl:ObjectProperty rdf:about="https://w3id.org/reno#hasGlazingType">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/reno#WindowGlazing"/>
        <rdfs:range rdf:resource="https://w3id.org/reno#WindowGlazingType"/>
        <rdfs:comment xml:lang="nl">De relatie tussen raambeglazing en het type.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship between window glazing and its type.</rdfs:comment>
        <rdfs:label xml:lang="en">has glazing type</rdfs:label>
        <rdfs:label xml:lang="nl">heeft beglazingstype</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reno#hasHeatingPeriod -->


    <owl:ObjectProperty rdf:about="https://w3id.org/reno#hasHeatingPeriod">
        <owl:inverseOf rdf:resource="https://w3id.org/reno#isHeatingPeriodOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Space"/>
        <rdfs:range rdf:resource="https://w3id.org/reno#HeatingPeriod"/>
        <rdfs:comment xml:lang="nl">De relatie tussen een ruimte en zijn verwarmingsperiode.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship between a space and its heating period.</rdfs:comment>
        <rdfs:label xml:lang="en">has heating period</rdfs:label>
        <rdfs:label xml:lang="nl">heeft verwarmingsperiode</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reno#hasInsulationGrade -->


    <owl:ObjectProperty rdf:about="https://w3id.org/reno#hasInsulationGrade">
        <owl:inverseOf rdf:resource="https://w3id.org/reno#isInsulationGradeOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://pi.pauwel.be/voc/buildingelement#BuildingElement"/>
        <rdfs:range rdf:resource="https://w3id.org/reno#InsulationGrade"/>
        <rdfs:comment xml:lang="nl">De relatie tussen een gebouwelement en de mate waarin het geïsoleerd is.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship between a building element and its grade of insulation.</rdfs:comment>
        <rdfs:label xml:lang="en">has insulation grade</rdfs:label>
        <rdfs:label xml:lang="nl">heeft isolatiegraad</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reno#hasMotivation -->


    <owl:ObjectProperty rdf:about="https://w3id.org/reno#hasMotivation">
        <owl:inverseOf rdf:resource="https://w3id.org/reno#isMotivationOf"/>
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/spec/Person"/>
        <rdfs:range rdf:resource="https://w3id.org/reno#Motivation"/>
        <rdfs:comment xml:lang="nl">De relatie tussen een persoon en hun motivatie.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship between a person and their motivation.</rdfs:comment>
        <rdfs:label xml:lang="en">has motivation</rdfs:label>
        <rdfs:label xml:lang="nl">heeft motivatie</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reno#hasOrientation -->


    <owl:ObjectProperty rdf:about="https://w3id.org/reno#hasOrientation">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Space"/>
        <rdfs:range rdf:resource="https://w3id.org/reno#Orientation"/>
        <rdfs:comment xml:lang="nl">De relatie tussen een ruimte en zijn oriëntatie.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship between a space and the orientation it faces.</rdfs:comment>
        <rdfs:label xml:lang="en">orientation</rdfs:label>
        <rdfs:label xml:lang="nl">oriëntatie</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reno#hasOwner -->


    <owl:ObjectProperty rdf:about="https://w3id.org/reno#hasOwner">
        <owl:inverseOf rdf:resource="https://w3id.org/reno#isOwnerOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Zone"/>
        <rdfs:range rdf:resource="https://w3id.org/reno#Owner"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/bot#containsZone"/>
            <rdf:Description rdf:about="https://w3id.org/reno#hasOwner"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="nl">De relatie tussen een zone en de persoon die het bezit.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship between a zone and the person that owns it.</rdfs:comment>
        <rdfs:label xml:lang="en">has owner</rdfs:label>
        <rdfs:label xml:lang="nl">heeft eigenaar</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reno#hasPlannedSpaceUsage -->


    <owl:ObjectProperty rdf:about="https://w3id.org/reno#hasPlannedSpaceUsage">
        <rdfs:domain rdf:resource="https://w3id.org/bot#Space"/>
        <rdfs:range rdf:resource="https://w3id.org/reno#SpaceUsage"/>
        <rdfs:comment xml:lang="nl">De relatie tussen een ruimte en zijn gepland type gebruik.</rdfs:comment>
