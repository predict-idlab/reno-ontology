        <rdfs:label xml:lang="en">renovation</rdfs:label>
        <rdfs:seeAlso xml:lang="en">https://en.wikipedia.org/wiki/Renovation</rdfs:seeAlso>
        <rdfs:seeAlso xml:lang="nl">https://nl.wikipedia.org/wiki/Renovatie</rdfs:seeAlso>
