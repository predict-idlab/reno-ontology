                    rdfs:domain :Tender ;
                    rdfs:range <https://w3id.org/bot#Element> ;
                    rdfs:comment "De relatie tussen een offerte en de (bouw)elementen die erdoor worden beïnvloed."@nl ,
                                 "The relationship between a tender and the (building) elements it affects."@en ;
                    rdfs:label "has affected element"@en ,
                               "heeft invloed op element"@nl .


