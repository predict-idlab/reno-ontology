###  http://purl.org/dc/terms/abstract
<http://purl.org/dc/terms/abstract> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/contributor
<http://purl.org/dc/terms/contributor> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
<http://purl.org/dc/terms/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
<http://purl.org/vocab/vann/preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
<http://purl.org/vocab/vann/preferredNamespaceUri> rdf:type owl:AnnotationProperty .


###  https://w3id.org/reno#hasNuisanceLevel
:hasNuisanceLevel rdf:type owl:AnnotationProperty ;
                  rdfs:comment "Geeft het overlastniveau van een renovatiesuggestie aan."@nl ,
                               "Indicates the nuisance level of a renovation suggestion."@en ;
                  rdfs:label "has nuisance level"@en ,
                             "heeft overlastniveau"@nl ;
                  rdfs:range rdfs:Literal ;
                  rdfs:domain :RenovationSuggestion .


###  https://w3id.org/reno#hasPointOfAttention
:hasPointOfAttention rdf:type owl:AnnotationProperty ;
                     rdfs:comment "De relatie tussen een renovatie suggestie en zijn aandachtspunt(en)."@nl ,
                                  "The relationship between a renovation suggestion and its point(s) of attention."@en ;
                     rdfs:label "has point of attention"@en ,
                                "heeft aandachtspunt"@nl ;
                     rdfs:range rdfs:Literal ;
                     rdfs:domain :RenovationSuggestion .


###  https://w3id.org/reno#hasUnitPrice
:hasUnitPrice rdf:type owl:AnnotationProperty ;
              rdfs:comment "Geeft de eenheidsprijs aan voor de renovatiesuggestie. Dit kan per stuk of per vierkante meter zijn, afhankelijk van het type renovatie."@nl ,
                           "Indicates the unit price for the renovation suggestion. This can be per piece or per sqm, depending on the type of renovation."@en ;
              rdfs:label "has unit price"@en ,
                         "heeft eenheidsprijs"@nl ;
              rdfs:range xsd:decimal ;
              rdfs:domain :RenovationSuggestion .


###  https://w3id.org/reno#permitNeeded
:permitNeeded rdf:type owl:AnnotationProperty ;
              rdfs:comment "Geeft aan of een omgevingsvergunning nodig is voor de renovatiesuggestie."@nl ,
                           "Indicates whether or not a permit is required for the renovation suggestion."@en ;
              rdfs:label "permit needed"@en ,
                         "vergunning nodig"@nl ;
              rdfs:range rdfs:Literal ;
              rdfs:domain :RenovationSuggestion .


###  https://w3id.org/reno#requiresMultiPhase
:requiresMultiPhase rdf:type owl:AnnotationProperty ;
                    rdfs:comment "Geeft aan of een renovatiesuggestie een meerfasige elektriciteitsaansluiting vereist."@nl ,
                                 "Indicates whether or not a renovation suggestion requires a multi-phase electricity connection."@en ;
                    rdfs:label "requires multi-phase"@en ,
                               "vereist meerfasig"@nl ;
                    rdfs:range rdfs:Literal ;
                    rdfs:domain :RenovationSuggestion .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#gYear
xsd:gYear rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#time
xsd:time rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/reno#changedDuring
:changedDuring rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               rdfs:range :RenovationIntervention ;
               rdfs:comment "De relatie tussen een bouwcomponent (set materiaallagen, raamkader...) en de renovatie interventie waarbij het veranderde."@nl ,
                            "The relationship between a building component (material layer set, windowframe...) and the renovation intervention during which it changed."@en ;
               rdfs:label "changed during"@en ,
                          "veranderd tijdens"@nl .


###  https://w3id.org/reno#changedInto
:changedInto rdf:type owl:ObjectProperty ;
             rdfs:comment "De relatie tussen een bouwdeel (set materiaallagen, raamkader...) en het bouwdeel waarin het veranderde."@nl ,
                          "The relationship between a building component (material layer set, windowframe...) and the building component it changed into."@en ;
             rdfs:label "changed into"@en ,
                        "veranderd in"@nl .


###  https://w3id.org/reno#clustering
:clustering rdf:type owl:ObjectProperty ;
            rdfs:domain <http://xmlns.com/foaf/spec/Person> ;
            rdfs:range :Persona ;
            rdfs:comment "Links a person to their clustered persona."@en ,
                         "Linkt een persoon aan zijn geclusterde persona."@nl ;
            rdfs:label "clustering"@en ,
                       "clustering"@nl .


###  https://w3id.org/reno#commodityAccomplishes
:commodityAccomplishes rdf:type owl:ObjectProperty ;
                       rdfs:domain <https://saref.etsi.org/core/Commodity> ;
                       rdfs:range <https://saref.etsi.org/core/Task> ;
                       owl:propertyChainAxiom ( <https://saref.etsi.org/core/isConsumedBy>
                                                <https://saref.etsi.org/core/accomplishes>
                                              ) ;
                       rdfs:comment "De relatie tussen een drager en de taak die erdoor wordt verwezenlijkt."@nl ,
                                    "The relationship between a commodity and the task it accomplishes."@en ;
                       rdfs:label "commodity accomplishes"@en ,
                                  "drager verwezenlijkt"@nl .


###  https://w3id.org/reno#forRenovationIntervention
:forRenovationIntervention rdf:type owl:ObjectProperty ;
                           rdfs:domain :Tender ;
                           rdfs:range :RenovationIntervention ;
                           rdfs:comment "Links a tender to the renovation intervention to which the tender is related."@en ,
                                        "Linkt een offerte aan de renovatie interventie waarop de offerte betrekking heeft."@nl ;
                           rdfs:label "for renovation intervention"@en ,
                                      "voor renovatie interventie"@nl .


###  https://w3id.org/reno#hasAddress
:hasAddress rdf:type owl:ObjectProperty ;
            owl:inverseOf :isAddressOf ;
            rdfs:domain :Resident ;
            rdfs:range :Address ;
            owl:propertyChainAxiom ( :isResidentOf
                                     :hasAddress
                                   ) ;
            rdfs:comment "De relatie tussen een persoon en het adres waarop ze wonen."@nl ,
                         "The relationship between a person and the address they reside at."@en ;
            rdfs:label "has address"@en ,
                       "heeft adres"@nl .


