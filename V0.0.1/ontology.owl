    <!-- http://bimerr.iot.linkeddata.es/def/material-properties#hasMaterial -->


    <owl:ObjectProperty rdf:about="http://bimerr.iot.linkeddata.es/def/material-properties#hasMaterial">
        <rdfs:range rdf:resource="http://bimerr.iot.linkeddata.es/def/material-properties#Material"/>
        <rdfs:comment>Relation between a construction component (Layer, profile, constituent) and the materials that define them.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://bimerr.iot.linkeddata.es/def/material-properties#hasMaterialLayer -->


    <owl:ObjectProperty rdf:about="http://bimerr.iot.linkeddata.es/def/material-properties#hasMaterialLayer">
        <rdfs:domain rdf:resource="http://bimerr.iot.linkeddata.es/def/material-properties#MaterialLayerSet"/>
        <rdfs:range rdf:resource="http://bimerr.iot.linkeddata.es/def/material-properties#MaterialLayer"/>
        <rdfs:comment>A relation between a layer set and the specific material layers.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://bimerr.iot.linkeddata.es/def/material-properties#hasMaterialLayerSet -->


    <owl:ObjectProperty rdf:about="http://bimerr.iot.linkeddata.es/def/material-properties#hasMaterialLayerSet">
        <rdfs:range rdf:resource="http://bimerr.iot.linkeddata.es/def/material-properties#MaterialLayerSet"/>
        <rdfs:comment>Relation between an element and its construction based in material layers.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.daml.org/services/owl-s/1.1/Process.owl#composedOf -->


    <owl:ObjectProperty rdf:about="http://www.daml.org/services/owl-s/1.1/Process.owl#composedOf">
        <rdfs:domain rdf:resource="http://www.daml.org/services/owl-s/1.1/Process.owl#CompositeProcess"/>
        <rdfs:range rdf:resource="http://www.daml.org/services/owl-s/1.1/Process.owl#ControlConstruct"/>
    </owl:ObjectProperty>
    


    <!-- http://www.daml.org/services/owl-s/1.1/Process.owl#hasPrecondition -->


    <owl:ObjectProperty rdf:about="http://www.daml.org/services/owl-s/1.1/Process.owl#hasPrecondition">
        <rdfs:domain rdf:resource="http://www.daml.org/services/owl-s/1.1/Process.owl#Process"/>
        <rdfs:range rdf:resource="http://www.daml.org/services/owl-s/1.1/generic/Expression.owl#Condition"/>
    </owl:ObjectProperty>
    


    <!-- http://www.daml.org/services/owl-s/1.1/Process.owl#hasResult -->


    <owl:ObjectProperty rdf:about="http://www.daml.org/services/owl-s/1.1/Process.owl#hasResult">
        <rdfs:domain rdf:resource="http://www.daml.org/services/owl-s/1.1/Process.owl#Process"/>
        <rdfs:range rdf:resource="http://www.daml.org/services/owl-s/1.1/Process.owl#Result"/>
        <rdfs:label>hasResult</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.daml.org/services/owl-s/1.1/Process.owl#process -->


    <owl:ObjectProperty rdf:about="http://www.daml.org/services/owl-s/1.1/Process.owl#process">
        <rdfs:domain rdf:resource="http://www.daml.org/services/owl-s/1.1/Process.owl#Perform"/>
        <rdfs:range rdf:resource="http://www.daml.org/services/owl-s/1.1/Process.owl#Process"/>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/core/accomplishes -->


    <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/accomplishes">
        <owl:inverseOf rdf:resource="https://saref.etsi.org/core/isAccomplishedBy"/>
        <rdfs:range rdf:resource="https://saref.etsi.org/core/Task"/>
        <rdfs:comment>Links a certain entity (e.g., a device) to the task it accomplishes.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/core/consumes -->


    <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/consumes">
        <owl:inverseOf rdf:resource="https://saref.etsi.org/core/isConsumedBy"/>
        <rdfs:comment>Links a feature kind, feature of interest, or device, to the commodity it consumes</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/core/isAccomplishedBy -->


    <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/isAccomplishedBy">
        <rdfs:comment>A relationship identifying an entity (e.g., a device) that can accomplish a task.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/core/isConsumedBy -->


    <owl:ObjectProperty rdf:about="https://saref.etsi.org/core/isConsumedBy">
        <rdfs:comment>Links a commodity to the feature kind, feature of interest, or device, that consumes it</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bot#adjacentElement -->


    <owl:ObjectProperty rdf:about="https://w3id.org/bot#adjacentElement">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/bot#hasElement"/>
        <owl:propertyDisjointWith rdf:resource="https://w3id.org/bot#intersectingElement"/>
        <rdfs:comment xml:lang="nl">Relatie tussen een zone en zijn aangrenzende gebouwelementen. Het gebouwelement begrenst de zone.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relation between a zone and its adjacent building elements, bounding the zone.</rdfs:comment>
        <rdfs:label xml:lang="nl">aangrenzend element</rdfs:label>
        <rdfs:label xml:lang="en">adjacent element</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bot#adjacentZone -->


    <owl:ObjectProperty rdf:about="https://w3id.org/bot#adjacentZone">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Zone"/>
        <rdfs:range rdf:resource="https://w3id.org/bot#Zone"/>
        <owl:propertyDisjointWith rdf:resource="https://w3id.org/bot#intersectsZone"/>
        <rdfs:comment xml:lang="nl">Relatie tussen twee zones die een interface delen, maar niet doorsnijdend zijn</rdfs:comment>
        <rdfs:comment xml:lang="en">Relationship between two zones that share a common interface, but do not intersect.</rdfs:comment>
        <rdfs:label xml:lang="nl">aangrenzende zone</rdfs:label>
        <rdfs:label xml:lang="en">adjacent zone</rdfs:label>
        <schema:domainIncludes rdf:resource="https://w3id.org/bot#Building"/>
        <schema:domainIncludes rdf:resource="https://w3id.org/bot#Site"/>
        <schema:domainIncludes rdf:resource="https://w3id.org/bot#Space"/>
        <schema:domainIncludes rdf:resource="https://w3id.org/bot#Storey"/>
        <schema:rangeIncludes rdf:resource="https://w3id.org/bot#Building"/>
        <schema:rangeIncludes rdf:resource="https://w3id.org/bot#Site"/>
        <schema:rangeIncludes rdf:resource="https://w3id.org/bot#Space"/>
        <schema:rangeIncludes rdf:resource="https://w3id.org/bot#Storey"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bot#containsElement -->


    <owl:ObjectProperty rdf:about="https://w3id.org/bot#containsElement">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/bot#hasElement"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/bot#containsZone"/>
            <rdf:Description rdf:about="https://w3id.org/bot#containsElement"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="nl">Relatie tussen zone en een gebouwelement bevat in die zone.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relation to a building element contained in a zone.</rdfs:comment>
        <rdfs:label xml:lang="nl">bevat element</rdfs:label>
        <rdfs:label xml:lang="en">contains element</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bot#containsZone -->


    <owl:ObjectProperty rdf:about="https://w3id.org/bot#containsZone">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Zone"/>
        <rdfs:range rdf:resource="https://w3id.org/bot#Zone"/>
        <rdfs:comment xml:lang="nl">Relatie tussen subzones van een hoofd zone. Een ruimtezone kan bijvoorbeeld worden bevat door een verdiepingszone, die wederom bevat wordt door een gebouwzone. bot:containsZone is een transitieve eigenschap, wat betekent dat in het vorige voorbeeld de ruimtezone ook bevat wordt door de gebouwzone.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relationship to the subzones of a major zone. A space zone could for instance be contained in a storey zone which is further contained in a building zone. bot:containsZone is a transitive property. This implies that in the previous example a bot:containsZone relationship holds between the space zone and the building zone.</rdfs:comment>
        <rdfs:label xml:lang="nl">bevat zone</rdfs:label>
        <rdfs:label xml:lang="en">contains zone</rdfs:label>
        <schema:domainIncludes rdf:resource="https://w3id.org/bot#Building"/>
        <schema:domainIncludes rdf:resource="https://w3id.org/bot#Site"/>
        <schema:domainIncludes rdf:resource="https://w3id.org/bot#Space"/>
        <schema:domainIncludes rdf:resource="https://w3id.org/bot#Storey"/>
        <schema:rangeIncludes rdf:resource="https://w3id.org/bot#Building"/>
        <schema:rangeIncludes rdf:resource="https://w3id.org/bot#Site"/>
        <schema:rangeIncludes rdf:resource="https://w3id.org/bot#Space"/>
        <schema:rangeIncludes rdf:resource="https://w3id.org/bot#Storey"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bot#hasBuilding -->


    <owl:ObjectProperty rdf:about="https://w3id.org/bot#hasBuilding">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/bot#containsZone"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Zone"/>
        <rdfs:range rdf:resource="https://w3id.org/bot#Building"/>
        <rdfs:comment xml:lang="nl">Relatie tot gebouwen die zich in een zone bevinden. De typische domeinen van bot:hasBuilding zijn instanties van bot:Site</rdfs:comment>
        <rdfs:comment xml:lang="en">Relation to buildings contained in a zone. The typical domains of bot:hasBuilding are instances of bot:Site.</rdfs:comment>
        <rdfs:label xml:lang="en">has building</rdfs:label>
        <rdfs:label xml:lang="nl">heeft gebouw</rdfs:label>
        <schema:domainIncludes rdf:resource="https://w3id.org/bot#Site"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bot#hasElement -->


    <owl:ObjectProperty rdf:about="https://w3id.org/bot#hasElement">
        <rdfs:domain rdf:resource="https://w3id.org/bot#Zone"/>
        <rdfs:range rdf:resource="https://w3id.org/bot#Element"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/bot#containsZone"/>
            <rdf:Description rdf:about="https://w3id.org/bot#hasElement"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Links a Zone to an Element that is either contained in, adjacent to or intersecting with the Zone. The intended use of this relationship is not to be stated explicitly, but to be inferred from its sub-properties. It will, for example, allow one to query for all the doors of a building given that they have an adjacency to spaces of the building.</rdfs:comment>
        <rdfs:comment xml:lang="nl">Verbindt een bot:Zone instantie met een bot:Element instantie die er ofwel door bevat wordt, aangrenzend aan is of snijdend mee is. Het is aangeraden om deze relatie niet expliciet te maken, maar af te leiden uit het gebruik van zijn sub relaties. Deze relatie maakt het bijvoorbeeld mogelijk om alle deuren van een gebouw op te vragen als die aangrenzend gedefinieerd zijn aan de ruimtes van het gebouw.</rdfs:comment>
        <rdfs:label xml:lang="en">has element</rdfs:label>
        <rdfs:label xml:lang="nl">heeft element</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bot#hasSpace -->


    <owl:ObjectProperty rdf:about="https://w3id.org/bot#hasSpace">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/bot#containsZone"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Zone"/>
        <rdfs:range rdf:resource="https://w3id.org/bot#Space"/>
        <rdfs:comment xml:lang="nl">Relatie tot ruimtes die zich in een zone bevinden. De typische domeinen van bot:hasSpace zijn instanties van bot:Storey of bot:Building.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relation to spaces contained in a zone. The typical domains of bot:hasSpace are instances of bot:Storey or bot:Building.</rdfs:comment>
        <rdfs:label xml:lang="en">has space</rdfs:label>
        <rdfs:label xml:lang="nl">heeft ruimte</rdfs:label>
        <schema:domainIncludes rdf:resource="https://w3id.org/bot#Storey"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bot#hasStorey -->


    <owl:ObjectProperty rdf:about="https://w3id.org/bot#hasStorey">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/bot#containsZone"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Zone"/>
        <rdfs:range rdf:resource="https://w3id.org/bot#Storey"/>
        <rdfs:comment xml:lang="nl">Relatie tot de verdiepingen die zich in een zone bevinden. De typische domeinen van bot:hasStorey zijn instanties van bot:Building.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relation to storeys contained in a zone. The typical domains of bot:hasStorey are instances of bot:Building.</rdfs:comment>
        <rdfs:label xml:lang="en">has storey</rdfs:label>
        <rdfs:label xml:lang="nl">heeft verdieping</rdfs:label>
        <schema:domainIncludes rdf:resource="https://w3id.org/bot#Building"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bot#hasSubElement -->


    <owl:ObjectProperty rdf:about="https://w3id.org/bot#hasSubElement">
        <rdfs:domain rdf:resource="https://w3id.org/bot#Element"/>
        <rdfs:range rdf:resource="https://w3id.org/bot#Element"/>
        <rdfs:comment xml:lang="nl">Relatie tussen twee gebouwelementen, ofwel één element dat een andere element bevat (bv. een muur die een raam bevat) of een subcompositie van een gebouwelement in kleinere delen (bv. een luchtbehandelingskast die een ventilator bevat).</rdfs:comment>
        <rdfs:comment xml:lang="en">Relation between two building elements, either one element hosting another (e.g. a wall hosts a window) or a subcomposition of a building element into smaller parts (e.g. an air handling unit has as a part a fan).</rdfs:comment>
        <rdfs:label xml:lang="en">has sub-element</rdfs:label>
        <rdfs:label xml:lang="nl">heeft subelement</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bot#intersectingElement -->


    <owl:ObjectProperty rdf:about="https://w3id.org/bot#intersectingElement">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/bot#hasElement"/>
        <rdfs:comment xml:lang="nl">Relatie tussen een zone en een gebouwelement die de zone doorsnijdt.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relation between a Zone and a building Element that intersects it.</rdfs:comment>
        <rdfs:label xml:lang="nl">doorsnijdend element</rdfs:label>
        <rdfs:label xml:lang="en">intersecting element</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/bot#intersectsZone -->


    <owl:ObjectProperty rdf:about="https://w3id.org/bot#intersectsZone">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/bot#Zone"/>
        <rdfs:range rdf:resource="https://w3id.org/bot#Zone"/>
        <rdfs:comment xml:lang="nl">Een relatie tussen twee zones waarvan de driedimensionale omvang doorsnijdend is. Bijvoorbeeld een traphal die verschillende verdiepingen doorsnijdt.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relationship between two zones whose 3D extent intersect. For example, a stairwell intersects different storeys.</rdfs:comment>
        <rdfs:label xml:lang="en">intersects zone</rdfs:label>
        <rdfs:label xml:lang="nl">snijdt door zone</rdfs:label>
        <schema:domainIncludes rdf:resource="https://w3id.org/bot#Building"/>
        <schema:domainIncludes rdf:resource="https://w3id.org/bot#Site"/>
        <schema:domainIncludes rdf:resource="https://w3id.org/bot#Space"/>
        <schema:domainIncludes rdf:resource="https://w3id.org/bot#Storey"/>
        <schema:rangeIncludes rdf:resource="https://w3id.org/bot#Building"/>
        <schema:rangeIncludes rdf:resource="https://w3id.org/bot#Site"/>
        <schema:rangeIncludes rdf:resource="https://w3id.org/bot#Space"/>
        <schema:rangeIncludes rdf:resource="https://w3id.org/bot#Storey"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reno#changedDuring -->


    <owl:ObjectProperty rdf:about="https://w3id.org/reno#changedDuring">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/reno#RenovationIntervention"/>
        <rdfs:comment xml:lang="nl">De relatie tussen een bouwcomponent (set materiaallagen, raamkader...) en de renovatie interventie waarbij het veranderde.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship between a building component (material layer set, windowframe...) and the renovation intervention during which it changed.</rdfs:comment>
        <rdfs:label xml:lang="en">changed during</rdfs:label>
        <rdfs:label xml:lang="nl">veranderd tijdens</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reno#changedInto -->


    <owl:ObjectProperty rdf:about="https://w3id.org/reno#changedInto">
        <rdfs:comment xml:lang="nl">De relatie tussen een bouwdeel (set materiaallagen, raamkader...) en het bouwdeel waarin het veranderde.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship between a building component (material layer set, windowframe...) and the building component it changed into.</rdfs:comment>
        <rdfs:label xml:lang="en">changed into</rdfs:label>
        <rdfs:label xml:lang="nl">veranderd in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reno#commodityAccomplishes -->


    <owl:ObjectProperty rdf:about="https://w3id.org/reno#commodityAccomplishes">
        <rdfs:domain rdf:resource="https://saref.etsi.org/core/Commodity"/>
        <rdfs:range rdf:resource="https://saref.etsi.org/core/Task"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://saref.etsi.org/core/isConsumedBy"/>
            <rdf:Description rdf:about="https://saref.etsi.org/core/accomplishes"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="nl">De relatie tussen een drager en de taak die erdoor wordt verwezenlijkt.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship between a commodity and the task it accomplishes.</rdfs:comment>
        <rdfs:label xml:lang="en">commodity accomplishes</rdfs:label>
        <rdfs:label xml:lang="nl">drager verwezenlijkt</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reno#hasAddress -->


    <owl:ObjectProperty rdf:about="https://w3id.org/reno#hasAddress">
        <rdfs:domain rdf:resource="https://w3id.org/reno#Resident"/>
        <rdfs:range rdf:resource="https://w3id.org/reno#Address"/>
