    <!-- https://schema.org/domainIncludes -->


    <owl:AnnotationProperty rdf:about="https://schema.org/domainIncludes">
        <rdfs:comment>Relates a property to a class that is (one of) the type(s) the property is expected to be used on.</rdfs:comment>
        <rdfs:label>domainIncludes</rdfs:label>
        <schema:domainIncludes rdf:resource="https://schema.org/Property"/>
        <schema:rangeIncludes rdf:resource="https://schema.org/Class"/>
    </owl:AnnotationProperty>
    


    <!-- https://schema.org/rangeIncludes -->


    <owl:AnnotationProperty rdf:about="https://schema.org/rangeIncludes">
        <rdfs:comment>Relates a property to a class that constitutes (one of) the expected type(s) for values of the property.</rdfs:comment>
        <rdfs:label>rangeIncludes</rdfs:label>
        <schema:domainIncludes rdf:resource="https://schema.org/Property"/>
        <schema:rangeIncludes rdf:resource="https://schema.org/Class"/>
    </owl:AnnotationProperty>
    


