                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">500</xsd:minInclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="nl">Het F label geeft energieprestatiecertificaten aan met een energiescore hoger dan 500 kWh/m²year.</rdfs:comment>
        <rdfs:comment xml:lang="en">The F label indicates energy performance certificates which have an energy score of more than 500 kWh/m²year.</rdfs:comment>
        <rdfs:label xml:lang="en">F label</rdfs:label>
        <rdfs:label xml:lang="nl">F label</rdfs:label>
    </owl:Class>
    


