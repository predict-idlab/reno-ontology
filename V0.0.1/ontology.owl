<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/reno#"
     xml:base="https://w3id.org/reno"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:reno="https://w3id.org/reno#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:schema="https://schema.org/"
     xmlns:Process="http://www.daml.org/services/owl-s/1.1/Process.owl#">
    <owl:Ontology rdf:about="https://w3id.org/reno#">
        <terms:abstract xml:lang="nl">Deze ontologie werd ontwikkeld als onderdeel van het RE-ENNOVATE project. Het doel is om concepten vast te leggen, die relevant zijn bij het ontwerpend van energetische renovatie suggesties.</terms:abstract>
        <terms:abstract xml:lang="en">This ontology was developed as part of the RE-ENNOVATE project. It aims to capture concepts that are relevant when designing energy renovation suggestions.</terms:abstract>
        <terms:creator>Emma Nuyts</terms:creator>
        <terms:creator>Femke Ongenae</terms:creator>
        <terms:creator>Mathias De Brouwer</terms:creator>
        <terms:license>https://creativecommons.org/licenses/by/4.0/</terms:license>
        <terms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-03-06</terms:modified>
        <terms:title xml:lang="nl">Renovatie en ENergie Optimalisatie (RENO)</terms:title>
        <terms:title xml:lang="en">Renovation and ENergy Optimization (RENO)</terms:title>
        <vann:preferredNamespacePrefix>reno</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>https://w3id.org/reno#</vann:preferredNamespaceUri>
        <owl:versionInfo>0.0.1</owl:versionInfo>
    </owl:Ontology>
    


