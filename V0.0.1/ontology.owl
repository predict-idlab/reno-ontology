                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:maxExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</xsd:maxExclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="nl">Het A+ label geeft energieprestatiecertificaten aan met een energiescore lager dan 100 kWh/m²year.</rdfs:comment>
        <rdfs:comment xml:lang="en">The A+ label indicates energy performance certificates which have an energy score of less than 100 kWh/m²year.</rdfs:comment>
        <rdfs:label xml:lang="en">A+ label</rdfs:label>
